{
  "plugins": [
    "@emotion",
    ["import", { "libraryName": "antd", "libraryDirectory": "lib" }, "ant"],
    ["react-activation/babel"]
  ],
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "env": {
    "development": {
      "presets": []
    },
    "extract": {
      "plugins": [
        [
          "ttag",
          {
            "extract": {
              "output": "locales/metabase-frontend.pot"
            },
            "discover": ["t", "jt"],
            "numberedExpressions": true
          }
        ]
      ]
    }
  }
}
